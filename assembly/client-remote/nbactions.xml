<?xml version="1.0" encoding="UTF-8"?>
<actions>
    <action>
        <actionName>CUSTOM-RunClient - select</actionName>
        <displayName>RunClient with Selector</displayName>
        <goals>
            <goal>compile</goal>
            <goal>jfx:run</goal>
        </goals>
        <properties>
            <select>true</select>
        </properties>
    </action>
    <action>
            <actionName>CUSTOM-RunClient Sample Localhost</actionName>
            <displayName>RunClient Sample Localhost</displayName>
            <goals>
                <goal>compile</goal>
                <goal>jfx:run</goal>
            </goals>
            <properties>
                <jfx.runAppParameter>--url=http://localhost:8080/tomee/ejb</jfx.runAppParameter>
            </properties>
            <activatedProfiles>
                <activatedProfile>client-sample</activatedProfile>
            </activatedProfiles>
        </action>
</actions>
