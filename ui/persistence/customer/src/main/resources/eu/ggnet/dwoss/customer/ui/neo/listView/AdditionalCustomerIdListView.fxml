<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="rootVbox" spacing="5.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="eu.ggnet.dwoss.customer.ui.neo.listView.AdditionalCustomerIdListViewController">
    <children>
        <Separator />
        <HBox>
            <children>
                <Label fx:id="titleLabel" text="Label" />
                <Region minHeight="26.0" minWidth="20.0" HBox.hgrow="ALWAYS" />
                <ImageView fx:id="addImage" fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true" HBox.hgrow="NEVER" />
            </children>
        </HBox>
        <ListView fx:id="listView" VBox.vgrow="ALWAYS" />
    </children>
</VBox>
