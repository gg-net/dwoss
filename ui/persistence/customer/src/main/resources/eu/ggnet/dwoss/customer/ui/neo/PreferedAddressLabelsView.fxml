<BorderPane prefHeight="925.0" prefWidth="1332.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="eu.ggnet.dwoss.customer.ui.neo.PreferedAddressLabelsController">
    <center>
        <VBox prefHeight="572.0" prefWidth="969.0">
            <children>
                <Label prefHeight="39.0" prefWidth="144.0" style="-fx-font-weight: bold;" text="Rechnungsadresse" underline="true" />
                <FlowPane hgap="15.0" prefHeight="336.0" prefWidth="1139.0" vgap="5.0">
                    <children>
                        <ListView fx:id="invoiceAddressCompanyListView" prefHeight="411.0" prefWidth="304.0" />
                        <ListView fx:id="invoiceAddressContactListView" prefHeight="414.0" prefWidth="289.0" />
                        <ListView fx:id="invoiceAddressAddressListView" prefHeight="385.0" prefWidth="246.0" />
                  <VBox fx:id="invoiceAddressVBox" prefHeight="377.0" prefWidth="437.0">
                     <children>
                        <Button fx:id="invoiceAddressClearButton" mnemonicParsing="false" onAction="#handleInvoiceAddressClearButtonAction" prefHeight="36.0" prefWidth="207.0" text="Clear" />
                     </children>
                  </VBox>
                    </children>
                </FlowPane>
                <Label prefHeight="35.0" prefWidth="137.0" text="Lieferadresse" />
                <FlowPane hgap="15.0" prefHeight="317.0" prefWidth="1139.0" vgap="5.0">
                    <children>
                        <ListView fx:id="shippingAddressCompanyListView" prefHeight="405.0" prefWidth="305.0" />
                        <ListView fx:id="shippingAddressContactListView" prefHeight="401.0" prefWidth="291.0" />
                        <ListView fx:id="shippingAddressAddressListView" prefHeight="401.0" prefWidth="249.0" />
                        <VBox fx:id="shippingAddressVBox" prefHeight="377.0" prefWidth="437.0">
                            <children>
                                <Button fx:id="shippingAddressClearButton" mnemonicParsing="false" onAction="#handleShippingAddressClearButtonAction" prefHeight="36.0" prefWidth="207.0" text="Clear" />
                            </children>
                        </VBox>
                    </children>
                </FlowPane>
            </children>
        </VBox>
    </center>
    <bottom>
        <GridPane prefHeight="90.0" prefWidth="969.0" BorderPane.alignment="CENTER">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <Button fx:id="saveButton" mnemonicParsing="false" onAction="#handleSaveButtonAction" prefHeight="29.0" prefWidth="246.0" text="Speichern" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#handleCancelButtonAction" prefHeight="31.0" prefWidth="252.0" text="Abbrechen" GridPane.columnIndex="3" GridPane.rowIndex="1" />
