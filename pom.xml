        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-javadoc-plugin</artifactId>
                <version>2.10.4</version>
                <configuration>
                    <groups>
                        <group>
                            <title>Common Core + API's (EE/UI)</title>
                            <packages>eu.ggnet.dwoss.search.api*:eu.ggnet.dwoss.customer.api*:eu.ggnet.dwoss.rights.api*:eu.ggnet.dwoss.uniqueunit.api*:eu.ggnet.dwoss.common*:eu.ggnet.dwoss.configuration*:eu.ggnet.dwoss.rules*:eu.ggnet.dwoss.event*:eu.ggnet.dwoss.mandator*:eu.ggnet.dwoss.progress*</packages>
                        </group>
                        <group>
                            <title>Persistence - Customer (EE/UI)</title>
                            <packages>eu.ggnet.dwoss.customer*</packages>
                        </group>
                        <group>
                            <title>Persistence - Report (EE/UI)</title>
                            <packages>eu.ggnet.dwoss.report*</packages>
                        </group>
                        <group>
                            <title>Persistence - Rights (EE/UI)</title>
                            <packages>eu.ggnet.dwoss.rights*</packages>
                        </group>
                        <group>
                            <title>Persistence - Product Specification (EE)</title>
                            <packages>eu.ggnet.dwoss.spec*</packages>
                        </group>
                        <group>
                            <title>Persistence - UniqueUnit (EE)</title>
                            <packages>eu.ggnet.dwoss.uniqueunit*</packages>
                        </group>
                        <group>
                            <title>Persistence - Stock (EE/UI)</title>
                            <packages>eu.ggnet.dwoss.stock*</packages>
                        </group>
                        <group>
                            <title>Persistence/Extended - RedTape (EE/UI)</title>
                            <packages>eu.ggnet.dwoss.redtape*</packages>
                        </group>
                        <group>
                            <title>Extended - Receipt (EE/UI) - The receiptment of single units</title>
                            <packages>eu.ggnet.dwoss.receipt*</packages>
                        </group>
                        <group>
                            <title>Extended - Misc (EE/UI) - Unsorted Features</title>
                            <packages>eu.ggnet.dwoss.misc*</packages>
                        </group>
                        <group>
                            <title>Extended - Price (EE/UI) - GG-Net Price Engine Core</title>
                            <packages>eu.ggnet.dwoss.price*</packages>
                        </group>
                        <group>
                            <title>Extended - Login (UI) - Enables timeconstraint login/logout</title>
                            <packages>eu.ggnet.dwoss.login*</packages>
                        </group>
                        <group>
                            <title>Utility Libraries (EE/UI)</title>
                            <packages>eu.ggnet.dwoss.util*</packages>
                        </group>
                        <group>
                            <title>Assembly</title>
                            <packages>eu.ggnet.dwoss.assembly*</packages>
                        </group>
                        <group>
                            <title>Incubator - Simple Application Framework Toolkit</title>
                            <packages>eu.ggnet.saft*</packages>
                        </group>
                    </groups>
                    <overview>${project.build.directory}/html/overview.html</overview>
                    <show>private</show>
                    <javadocVersion>1.8</javadocVersion>
                    <doclet>org.umlgraph.doclet.UmlGraphDoc</doclet>
                    <docletArtifact>
                        <groupId>org.umlgraph</groupId>
                        <artifactId>umlgraph</artifactId>
                        <version>5.6.6</version>
                    </docletArtifact>
                    <!--Might Remove Xdoclint if all javadocs are clean -->
                    <additionalparam>-attributes -types -enumerations -enumconstants -Xdoclint:none -link https://docs.oracle.com/javase/8/docs/api/</additionalparam>
                    <!-- Describing the tags of umlgraph removes them as warning. -->
                    <tags>
                        <tag>
                            <name>has</name>
                            <placement>a</placement>
                            <head>Has:</head>
                        </tag>
                        <tag>
                            <name>composed</name>
                            <placement>a</placement>
                            <head>Composed:</head>
                        </tag>
                    </tags>
                    <!--<useStandardDocletOptions>true</useStandardDocletOptions>-->
                </configuration>
                <executions>
                    <execution>
                        <id>aggregate</id>
                        <goals>
                            <goal>aggregate</goal>
                        </goals>
                        <phase>site</phase>
                    </execution>
                </executions>
