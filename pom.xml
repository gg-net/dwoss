                    <footerHtmlFile>${project.basedir}/src/main/html/footer.html</footerHtmlFile>
                </configuration>
                <executions>
                    <execution>
                        <phase>process-sources</phase>
                        <goals>
                            <goal>generate</goal>
                        </goals>
                    </execution>
                </executions>
            </plugin>
